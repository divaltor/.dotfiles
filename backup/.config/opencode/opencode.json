{
  "plugin": [
    "oh-my-opencode",
    "opencode-antigravity-auth"
  ],
  "agent": {
    "build": {
      "disable": true
    },
    "plan": {
      "disable": true
    },
  },
  "provider": {
    "google": {
      "models": {
        "claude-opus-4-5-thinking-high": {
          "name": "Claude Opus 4.5 Thinking High (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
      }
    }
  },
  "experimental": {
    "openTelemetry": true
  },
  "default_agent": "minerva",
  "$schema": "https://opencode.ai/config.json"
}
