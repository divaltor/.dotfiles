{
    "workbench.colorTheme": "Rosé Pine Dawn",
    "workbench.iconTheme": "catppuccin-latte",
    "editor.fontSize": 14,
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
    "redhat.telemetry.enabled": false,
    "files.autoSave": "onFocusChange",
    "editor.minimap.enabled": false,
    "git.autofetch": true,
    "editor.lineNumbers": "relative",
    "errorLens.gutterIconSet": "circle",
    "errorLens.fontStyleItalic": true,
    "errorLens.gutterIconsEnabled": true,
    "errorLens.messageBackgroundMode": "message",
    "errorLens.messageTemplate": "$message ($code)",
    "errorLens.problemRangeDecorationEnabled": true,
    "search.useParentIgnoreFiles": true,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        }
    },
    "cursor.cpp.enablePartialAccepts": true,
    "cursor.composer.collapsePaneInputBoxPills": true,
    "cursor.general.enableShadowWorkspace": true,
    "python.testing.pytestEnabled": true,
    "terminal.external.osxExec": "Ghostty.app",
    "terminal.explorerKind": "external",
    "terminal.integrated.enableImages": true,
    "search.useIgnoreFiles": false,
    "search.quickOpen.includeSymbols": true,
    "search.quickOpen.history.filterSortOrder": "recency",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "catppuccin-icons.associations.folders": {
        ".flox": "folder_nix"
    },
    "cursor.cpp.disabledLanguages": [
        "markdown",
        "plaintext",
        "csv"
    ],
    "cursor.composer.shouldAllowCustomModes": true,
    "terminal.integrated.hideOnStartup": "whenEmpty",
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.env.osx": {
        "SHELL": "/opt/homebrew/bin/fish"
    },
    "workbench.sideBar.location": "right",
    "crashReporting.enabled": "off",
    "explorer.confirmPasteNative": false,
    "explorer.confirmDelete": false,
    "rust-analyzer.inlayHints.closureStyle": "rust_analyzer",
    "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "reborrow",
    "rust-analyzer.inlayHints.expressionAdjustmentHints.hideOutsideUnsafe": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "PERF"
    ],
    "todo-tree.highlights.backgroundColourScheme": [
        "indianred",
        "mediumaquamarine",
        "lightsalmon",
        "lightsteelblue",
        "peachpuff"
    ],
    "todo-tree.highlights.useColourScheme": true,
    "todo-tree.highlights.foregroundColourScheme": [
        "white",
        "black",
        "black",
        "black",
        "black",
        "white",
        "black"
    ],
    "editor.formatOnSave": true,
    "cursor.diffs.useCharacterLevelDiffs": true,
    "ruff.configurationPreference": "filesystemFirst",
    "python.locator": "native",
    "editor.formatOnSaveMode": "file",
    "editor.codeActionsOnSave": {
        "source.fixAll.biome": "always",
        "source.organizeImports.biome": "always"
    },
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.implementationsCodeLens.showOnInterfaceMethods": true,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.includePackageJsonAutoImports": "on",
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "zig.zls.enabled": "on",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Rosé Pine Dawn",
    "workbench.preferredHighContrastLightColorTheme": "Rosé Pine Dawn",
    "workbench.preferredDarkColorTheme": "Rosé Pine",
    "workbench.preferredHighContrastColorTheme": "Rosé Pine",
    "terminal.integrated.fontLigatures.enabled": true,
    "editor.fontLigatures": true,
    "cursorpyright.analysis.autoImportCompletions": true,
    "cursorpyright.analysis.diagnosticMode": "workspace",
    "cursorpyright.analysis.inlayHints.callArgumentNames": true,
    "cursorpyright.analysis.inlayHints.functionReturnTypes": true,
    "cursorpyright.analysis.inlayHints.variableTypes": true,
    "cursorpyright.analysis.typeCheckingMode": "strict",
    "ty.diagnosticMode": "workspace",
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "git.openRepositoryInParentFolders": "never",
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "editor.formatOnType": true,
    "ty.experimental.autoImport": true,
    "ty.experimental.rename": true,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "prisma.hidePrisma6Prompts": true,
    "amp.tab.enabled": true,
}